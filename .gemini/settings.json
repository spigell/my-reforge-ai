{
  "general": {
    "vimMode": true,
    "preferredEditor": "code",
    "checkpointing": {
      "enabled": true
    }
  },
  "ui": {
    "theme": "Dracula",
    "hideBanner": true,
    "hideTips": false
  },
  "tools": {
    "sandbox": "false",
    "autoAccept": "true"
  },
  "privacy": {
    "usageStatisticsEnabled": false
  },
  "model": {
    "name": "gemini-2.5-pro",
    "maxSessionTurns": 100
  },
  "context": {
    "fileName": [
      "AGENTS.md",
      "GEMINI.md"
    ],
    "fileFiltering": {
      "respectGitIgnore": true
    }
  },
  "advanced": {
    "excludedEnvVars": [
      "DEBUG",
      "DEBUG_MODE",
      "NODE_ENV"
    ]
  },
  "mcpServers": {
    "mcp-github": {
      "httpUrl": "http://mcp-github.my-reforge-ai.svc.cluster.local:8080/mcp/"
    }
  }
}